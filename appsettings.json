{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  
  "AllowedHosts": "*",

  "App": {
    "Python": {
      "Executable": "python",
      "Script": "D:/Postgres/Importer.py",
      "TimeoutSeconds": 7200
    },
    "Logs": {
      "Dir": "D:/Postgres/logs"
    },
    "Worker": {
      "Enabled": true,
      "PollMillis": 1000,
      "MaxConcurrent": 1
    }
  }
}
